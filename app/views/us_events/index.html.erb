<p id="notice"><%= notice %></p>

<h1>Us Events</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Start date</th>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @us_events.each do |us_event| %>
      <tr>
        <td><%= us_event.name %></td>
        <td><%= us_event.start_date %></td>
        <td><%= us_event.address %></td>
        <td><%= us_event.latitude %></td>
        <td><%= us_event.longitude %></td>
        <td><%= link_to 'Show', us_event %></td>
        <td><%= link_to 'Edit', edit_us_event_path(us_event) %></td>
        <td><%= link_to 'Destroy', us_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<h1><%= @us_events.size %></h1>

<%= link_to 'New Us Event', new_us_event_path %>
